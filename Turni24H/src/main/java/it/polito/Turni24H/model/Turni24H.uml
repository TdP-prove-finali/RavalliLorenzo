              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_JHwOellbEe6zgPim6hxQEQ" name="aggiungiTurni" specification="_JHlPVFlbEe6zgPim6hxQEQ">
                <ownedParameter xmi:id="_JHwOe1lbEe6zgPim6hxQEQ" name="parziale" type="_JHehoVlbEe6zgPim6hxQEQ"/>
                <ownedParameter xmi:id="_JHwOfFlbEe6zgPim6hxQEQ" name="richiestiOff">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#boolean"/>
                </ownedParameter>
                <ownedParameter xmi:id="_JHwOfVlbEe6zgPim6hxQEQ" name="weekend">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                </ownedParameter>
                <ownedParameter xmi:id="_JHwOfllbEe6zgPim6hxQEQ" name="straordinario">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                </ownedParameter>
                <language>java</language>
                <body>for(Turno t : turni) {
				if(turniCompletati(t, parziale)==false) {
					for(RichiestaTurni r : this.richiesteTurni) {
						if(r.isOffDay()==false &amp;&amp; r.getGiorno() == t.getGiorno() &amp;&amp; 
									r.getTipoTurno().compareTo(t.getTipoTurno()) == 0) {
							for(Staff s : allStaff) {
								if(s.getId() == r.getIdStaff()) {
									if(isDisponibile(parziale, s, t, richiestiOff, weekend, straordinario)
											&amp;&amp; turniCompletati(t, parziale)==false) {
										Accoppiamenti a = new Accoppiamenti(s,t);
										parziale.add(a);
										break;
									}
								}
							}
						}
					}
				}
			}
	</body>
