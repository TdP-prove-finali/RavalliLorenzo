              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_JHwOcllbEe6zgPim6hxQEQ" name="cercaLista" specification="_JG554FlbEe6zgPim6hxQEQ">
                <ownedParameter xmi:id="_JHwOc1lbEe6zgPim6hxQEQ" name="profondita">
                  <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
                </ownedParameter>
                <ownedParameter xmi:id="_JHwOdFlbEe6zgPim6hxQEQ" name="richiestiOff">
