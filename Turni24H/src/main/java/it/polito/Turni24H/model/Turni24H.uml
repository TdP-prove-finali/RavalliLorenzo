              <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_JHwOj1lbEe6zgPim6hxQEQ" name="turniCompletati" specification="_JHmdeFlbEe6zgPim6hxQEQ">
                <ownedParameter xmi:id="_JHwOkFlbEe6zgPim6hxQEQ" name="t" type="_JHCcwFlbEe6zgPim6hxQEQ"/>
                <ownedParameter xmi:id="_JHwOkVlbEe6zgPim6hxQEQ" name="parziale" type="_JHehoVlbEe6zgPim6hxQEQ"/>
